[app::Settings {
    page: {
        fit_mode: fit
        interpolation_mode: linear
    }
    window: {
        size: [720 720]
        fullscreen: false
    }
    memory: {
        preload_ahead: 1
        preload_behind: 1
        page_cache_mb: 200
    }
    mappings: [
        [[right] [next]]
        [[left] [prev]]
        [[shift right] [seek 20]]
        [[shift left] [seek -20]]
        [["="] [zoom_multiply 1.4142135623731]]
        [["-"] [zoom_multiply 0.707106781186547]]
        [[f11] [fullscreen]]
        [[f] [fit_mode fit]]
        [[s] [fit_mode stretch]]
        [[escape] [leave_fullscreen_or_quit]]
        [[ctrl q] [quit]]
        [[ctrl 1] [interpolation_mode nearest]]
        [[ctrl 2] [interpolation_mode linear]]
        #[[ctrl 3] [interpolation_mode cubic]]
    ]
}]
