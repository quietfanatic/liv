[app::Settings {
    default_view: {
        fit_mode: fit
        zoom: 1.0
    }
    mappings: [
        [[right] [next]]
        [[left] [prev]]
        [[ctrl q] [quit]]
        [[f11] [fullscreen]]
        [[f] [fit_mode fit]]
        [[s] [fit_mode stretch]]
        [[escape] [leave_fullscreen_or_quit]]
    ]
}]
